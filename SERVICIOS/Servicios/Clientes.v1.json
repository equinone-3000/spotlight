          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "tipodoc": {
                    "type": "string",
                    "description": "Tipo de documento Ej:pasaporte, Rut"
                  },
                  "rut": {
                    "type": "string",
                    "description": "Rut de Cliente persona o empresa"
                  },
                  "dv": {
                    "type": "string",
                    "description": "Digito Verificador asociado al Rut"
                  },
                  "identificacion": {
                    "type": "string",
                    "description": "Razon Social del Cliente"
                  },
                  "personaEmpresa": {
                    "type": "string",
                    "description": "Si cliente es Persona o Empresa"
                  },
                  "paisCodigo": {
                    "type": "integer",
                    "description": "Codigo del Pais de Origen del Cliente"
                  }
                },
                "required": [
                  "tipodoc",
                  "rut",
                  "dv",
                  "identificacion",
                  "personaEmpresa",
                  "paisCodigo"
                ]
              },
              "examples": {
                "Ejemplo": {
                  "value": {
                    "tipodoc": "1",
                    "rut": "99501480",
                    "dv": "7",
                    "identificacion": "PENTA FINANCIERO S.A",
                    "personaEmpresa": "EMPRESA",
                    "paisCodigo": 997
                  }
                }
              }
            }
          },
          "description": "Datos de basicos para creacion de cliente"
        }
      }
    },
